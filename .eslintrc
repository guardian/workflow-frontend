---
env:
    browser: true
    es6: true
globals:
    angular: false
    module: false
    _wfConfig: false
parser: babel-eslint
plugins:
    - angular
rules:
    no-unused-vars: 2
    no-debugger: 0
    semi: 0
    linebreak-style: 0
    no-bitwise: 2
    no-cond-assign: 0
    curly: 2
    eqeqeq: 2
    no-eq-null: 0
    no-eval: 0
    no-unused-expressions: 0
    guard-for-in: 0
    wrap-iife:
        - 2
        - any
    no-use-before-define:
        - 2
        - functions: false
    no-loop-func: 0
    no-caller: 2
    no-script-url: 2
    no-shadow: 0
    no-new-func: 0
    no-new-wrappers: 0
    no-undef: 2
    new-cap: 0
    no-empty: 2
    no-new: 2
    no-plusplus: 0
    dot-notation: 2
